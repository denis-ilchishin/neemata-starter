{
  "compilerOptions": {
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "target": "ESNext",
    "noImplicitAny": false,
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "declaration": false,
    "emitDeclarationOnly": false,
    "jsx": "preserve",
    "strictNullChecks": true,
    "allowImportingTsExtensions": true,
    "baseUrl": ".",
    "paths": {
      "#app": [
        "./application/application.ts"
      ],
      "#procedures": [
        "./application/procedures.ts"
      ],
      "#domain/*": [
        "./application/domain/*"
      ]
    }
  }
}